			"invalid": "تاريخ {field} {value} غير صالح"
		},
		"types": {
			"string": "{field} ليس {type}",
			"method": "{field} ليس {type} (وظيفة)",
			"array": "{field} ليس {type}",
			"object": "{field} ليس {type}",
			"number": "{field} ليس {type}",
			"date": "{field} ليس {type}",
			"boolean": "{field} ليس {type}",
			"integer": "{field} ليس {type}",
			"float": "{field} ليس {type}",
			"regexp": "{field} ليس {type} صالحًا",
			"email": "{field} ليس {type} صالحًا",
			"url": "{field} ليس {type} صالحًا",
			"hex": "{field} ليس {type} صالحًا"
		},
		"string": {
